{
    "name": "datlv/laravel-meta",
    "description": "Laravel Meta Package",
    "keywords": [
        "Datlv",
        "laravel",
        "meta"
    ],
    "homepage": "https://github.com/datlv/laravel-meta",
    "license": "MIT",
    "authors": [
        {
            "name": "Datlv",
            "email": "datle2011@gmail.com",
            "homepage": "http://Datlv.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/datlv/laravel-kit.git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "datlv/laravel-kit": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "orchestra/database": "~3.5",
        "nunomaduro/collision": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Datlv\\Meta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datlv\\Meta\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datlv\\Meta\\ServiceProvider"
            ]
        }
    }
}
